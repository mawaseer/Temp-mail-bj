{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/docs",
      "dest": "/public/docs.html"
    },
    {
      "src": "/getmail(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/chkmail(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/delete(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/",
      "dest": "/index.js"
    }
  ]
}
